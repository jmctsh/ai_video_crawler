{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist-main",
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["ES2020"],
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": false
  },
  "include": ["src/main/**/*.ts", "src/main/**/*.d.ts"]
}